<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.jk.mapper.personal.PersonalMapper">

<insert id="addUser">
    insert into t_user(userImg,userName,sincerityName,userPhone,userEmail,userSex,userBirthday)
    values(
    #{userImg},#{userName},#{sincerityName},#{userPhone},#{userEmail},#{userSex},#{userBirthday}
    )
</insert>

    <update id="updPassword">

        update t_user set userPassword={login.userPassword}
        where userId=#{id}
    </update>
</mapper>